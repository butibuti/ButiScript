/*ラムダ式内部でラムダ式を使えない問題アリ*/
SubFunction():void{
	print("SubFunction!!!\n");
	var subFunctionObj:()=>void;
	subFunctionObj=()=>void{
		print("SubRamdaFunction!!!\n");
		var subsubFunctionObj:(i: int)=>void;
		subsubFunctionObj=(i:int)=>void{
			print("SubRamdaFunction!!!\n");
			};
		subsubFunctionObj(0);
	};
	subFunctionObj();
	
}
main():int
{

	var function:()=>void;
	
	function=SubFunction;
	
	function();

	return 0;
	
}
